#! /usr/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1b843c0 .scope module, "ckl_div_tb" "ckl_div_tb" 2 3;
 .timescale 0 0;
v0x1b935f0_0 .net "clk", 0 0, v0x1b82f70_0; 1 drivers
v0x1b936c0_0 .var "mclk", 0 0;
S_0x1b844b0 .scope module, "clk_div" "clk_div" 2 7, 3 6, S_0x1b843c0;
 .timescale 0 0;
v0x1b82f70_0 .var "clk", 0 0;
v0x1b934b0_0 .var "cnt", 1 0;
v0x1b93550_0 .net "mclk", 0 0, v0x1b936c0_0; 1 drivers
E_0x1b60760 .event posedge, v0x1b93550_0;
    .scope S_0x1b844b0;
T_0 ;
    %set/v v0x1b82f70_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x1b844b0;
T_1 ;
    %set/v v0x1b934b0_0, 0, 2;
    %end;
    .thread T_1;
    .scope S_0x1b844b0;
T_2 ;
    %wait E_0x1b60760;
    %load/v 8, v0x1b934b0_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %set/v v0x1b934b0_0, 8, 2;
    %load/v 8, v0x1b934b0_0, 2;
    %nor/r 8, 8, 2;
    %jmp/0xz  T_2.0, 8;
    %load/v 8, v0x1b82f70_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x1b82f70_0, 0, 8;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1b843c0;
T_3 ;
    %set/v v0x1b936c0_0, 0, 1;
    %vpi_call 2 13 "$dumpfile", "clk_div_tb.vcd";
    %vpi_call 2 14 "$dumpvars";
    %delay 10000, 0;
    %vpi_call 2 15 "$finish";
    %end;
    .thread T_3;
    .scope S_0x1b843c0;
T_4 ;
    %delay 1, 0;
    %load/v 8, v0x1b936c0_0, 1;
    %inv 8, 1;
    %set/v v0x1b936c0_0, 8, 1;
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "clk_div_tb.v";
    "./clk_div.v";
